#aliases add source path on this file to ~/.bashrc like source /home/$USER/aliases
#Apache log
alias apachelog='tail -50 /var/log/apache2/error_log'
#Go to alias folder
alias www='cd /var/www/ && ls'
alias html='cd /var/www/html/'
alias $USER='cd /home/$USER/'
alias home='cd /home/'
alias ..='cd ..'
alias l0g='cd /var/log/ && ls'
alias ยง='cd / && ls'
alias repos="cd /home/$USER/git/ && ls"
alias ยง="cd  / && ls"
alias var="cd /var/"
#prv
alias backend='cd /var/www/html/sentinel/backend/ && ls'
alias frontend='cd /var/www/html/sentinel/frontend/ && ls' 
alias sentinel='cd /var/www/html/sentinel/ && ls' 